<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
                     xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:HeBianGu.General.WpfControlLib"
    xmlns:h="QQ:908293466">

    <Style x:Key="S.SingleTickBar.Default" TargetType="{x:Type local:SingleTickBar}">
        <Setter Property="Foreground" Value="{DynamicResource S.Brush.TextForeground.Default}"/>
        <Setter Property="SplitValue" Value="10"/>
        <Setter Property="Height" Value="{DynamicResource S.Window.Row.Height}"/>
    </Style>

    <Style TargetType="{x:Type local:SingleTickBar}" BasedOn="{StaticResource S.SingleTickBar.Default}"/>
</ResourceDictionary>
