 <ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"> 
    
    <!--窗口阴影效果-->
    <DropShadowEffect x:Key="S.DropShadowEffect.Window" 
                      Color="{DynamicResource AccentColor}"  
                      BlurRadius="8" 
                      ShadowDepth="0" 
                      Direction="0" 
                      Opacity="0.7" />

    <!--主色调-->
    <SolidColorBrush x:Key="S.Brush.Accent"  Color="{DynamicResource AccentColor}"/>

    <!--半透明的主色调 一般用于MouseOver-->
    <SolidColorBrush x:Key="S.Brush.Accent.MouseOver"  Color="{DynamicResource  AccentColor}" Opacity="0.5"/>
    <SolidColorBrush x:Key="S.Brush.Accent.Opacity.9"  Color="{DynamicResource  AccentColor}" Opacity="0.9"/>

    <!--半透明的主色调 一般用于MouseOver-->
    <SolidColorBrush x:Key="S.Brush.Accent.Opacity.8"  Color="{DynamicResource  AccentColor}" Opacity="0.8"/>
    <SolidColorBrush x:Key="S.Brush.Accent.Opacity.7"  Color="{DynamicResource  AccentColor}" Opacity="0.7"/>

    <!--半透明的主色调 一般用于MouseOver-->
    <SolidColorBrush x:Key="S.Brush.Accent.Opacity.6"  Color="{DynamicResource  AccentColor}" Opacity="0.6"/>

    <!--半透明的主色调 一般用于MouseOver-->
    <SolidColorBrush x:Key="S.Brush.Accent.Opacity.5"  Color="{DynamicResource  AccentColor}" Opacity="0.5"/>
    <SolidColorBrush x:Key="S.Brush.Accent.Opacity.4"  Color="{DynamicResource  AccentColor}" Opacity="0.4"/>

    <!--半透明的主色调 一般用于MouseOver-->
    <SolidColorBrush x:Key="S.Brush.Accent.Opacity.3"  Color="{DynamicResource  AccentColor}" Opacity="0.3"/>
    <SolidColorBrush x:Key="S.Brush.Accent.Opacity.2"  Color="{DynamicResource  AccentColor}" Opacity="0.2"/>

    <!--半透明的主色调 一般用于MouseOver-->
    <SolidColorBrush x:Key="S.Brush.Accent.Opacity.1"  Color="{DynamicResource  AccentColor}" Opacity="0.1"/>

    <!--磨砂窗体背景色调-->
    <SolidColorBrush x:Key="S.Brush.WindowBackground" Opacity="0.15" Color="{DynamicResource AccentColor}"/>

    <SolidColorBrush x:Key="S.Brush.Share.UnEnbled.Background" Color="#dcdcdc"/>

    <SolidColorBrush x:Key="S.Brush.Share.UnEnbled.Foreground" Color="#f8f8f8"/>
     
    <!--用于内容域、面板底色-->
    <SolidColorBrush x:Key="S.Brush.LightGray.Notice" Color="#f8f8f8"/>

    <!--用于透明背景内容域、面板底色-->
    <SolidColorBrush x:Key="S.Brush.LightGray.NoticeOpacity.5" Color="#f8f8f8" Opacity="0.5"/>

    <!--用于边框、分割线-->
    <SolidColorBrush x:Key="S.Brush.Gray.Notice" Color="#dcdcdc"></SolidColorBrush>
    
    <SolidColorBrush x:Key="S.Brush.Gray.Notice.Opacity.5" Color="#dcdcdc"  Opacity="0.5"/>

    <!--黄色  用于提示文字和图标-->
    <SolidColorBrush x:Key="S.Brush.Orange.Notice" Color="#f7b61f"/>

    <!--黑色 用于提示文字和图标-->
    <SolidColorBrush x:Key="S.Brush.Black.Notice" Color="#374857"/>

    <!--黑色 用于提示文字和图标-->
    <SolidColorBrush x:Key="S.Brush.Dark.10" Color="#000000"/>
    <SolidColorBrush x:Key="S.Brush.Dark.9" Color="#2c2c2c"/>
    <SolidColorBrush x:Key="S.Brush.Dark.8" Color="#515151"/>
    <SolidColorBrush x:Key="S.Brush.Dark.7" Color="#8a8a8a"/>
    <SolidColorBrush x:Key="S.Brush.Dark.6" Color="#707070"/>
    <SolidColorBrush x:Key="S.Brush.Dark.5" Color="#bfbfbf"/>
    <SolidColorBrush x:Key="S.Brush.Dark.4" Color="#cdcdcd"/>
    <SolidColorBrush x:Key="S.Brush.Dark.3" Color="#dbdbdb"/>
    <SolidColorBrush x:Key="S.Brush.Dark.2" Color="#e6e6e6"/>
    <SolidColorBrush x:Key="S.Brush.Dark.1" Color="#ffffff"/>
    <SolidColorBrush x:Key="S.Brush.Dark.0" Color="#959595"/>

    <!--红色 用于警告文字和图标-->
    <SolidColorBrush x:Key="S.Brush.Red.Notice" Color="#d81e06"/>
    <SolidColorBrush x:Key="S.Brush.Red.Notice.Opactiy.8" Color="#d81e06" Opacity="0.8"/>
    <SolidColorBrush x:Key="S.Brush.Red.Notice.Opactiy.6" Color="#d81e06" Opacity="0.6"/>
    <SolidColorBrush x:Key="S.Brush.Red.Notice.Opactiy.4" Color="#d81e06" Opacity="0.4"/>
    <SolidColorBrush x:Key="S.Brush.Red.Notice.Opactiy.2" Color="#d81e06" Opacity="0.2"/>

    <!--绿色 用于提示文字和图标-->
    <SolidColorBrush x:Key="S.Brush.Green.Notice" Color="#00CC00"/>

    <!--面板、标题、输入框底色-->
    <SolidColorBrush x:Key="S.Brush.White.Notice" Color="#fafafa"/>
    
    <SolidColorBrush x:Key="S.Brush.White.Opactiy.8" Color="#fafafa" Opacity="0.8"/>

    <SolidColorBrush x:Key="S.Brush.White.Opactiy.6" Color="#fafafa" Opacity="0.6"/>

    <SolidColorBrush x:Key="S.Brush.White.Opactiy.4" Color="#fafafa" Opacity="0.4"/>

    <SolidColorBrush x:Key="S.Brush.White.Opactiy.2" Color="#fafafa" Opacity="0.2"/>

    <SolidColorBrush x:Key="S.Brush.White.Opactiy.1" Color="#fafafa" Opacity="0.1"/>

    <!--重要文字信息及标题-->
    <SolidColorBrush x:Key="S.Brush.TextForeground.Tiltle" Color="#212121"/>

    <!--用于普通文字、内容信息-->
    <SolidColorBrush x:Key="S.Brush.TextForeground.Default" Color="#727272"/>

    <!--用于标注、辅助文字-->
    <SolidColorBrush x:Key="S.Brush.TextForeground.Assist" Color="#b6b6b6"/>

    <!--用于链接文字文字-->
    <SolidColorBrush x:Key="S.Brush.TextForeground.Link" Color="#6296d8"/>

    <!--用于面板底色内容域底色-->
    <SolidColorBrush x:Key="S.Brush.TextBackground.Default" Color="#f8f8f8"/>

    <!--用于边框、分割线-->
    <SolidColorBrush x:Key="S.Brush.TextBorderBrush.Default" Color="#dcdcdc"/>

     <!--换行显示的颜色-->
    <SolidColorBrush x:Key="S.Brush.RowIndex.BackGround" Color="#F0F5FB"/>

</ResourceDictionary>