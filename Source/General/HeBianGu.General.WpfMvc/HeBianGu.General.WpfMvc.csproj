<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
    <TargetFrameworks>netcoreapp3.0;net45</TargetFrameworks>
    <UseWPF>true</UseWPF>
    <AssemblyVersion>2.1.0</AssemblyVersion>
    <AssemblyFileVersion>2.1.0</AssemblyFileVersion>
    <AssemblyInformationVersion>2.1.0</AssemblyInformationVersion>
    <Version>2.1.0</Version>
    <Authors>HeBianGu</Authors>
    <Description>WPF MVC控件资源库</Description>
    <Copyright>Copyright 2019 HeBianGu. All rights reserved.</Copyright>
    <PackageIcon>logo.jpg</PackageIcon>
    <PackageIconUrl />
    <PackageProjectUrl>https://github.com/HeBianGu/WPF-ControlBase</PackageProjectUrl>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageTags>Wpf,Library,HeBianGu,WpfMvc</PackageTags>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(TargetFramework)|$(Platform)'=='Debug|netcoreapp3.0|AnyCPU'">
    <OutputPath>..\..\..\Product\Debug\</OutputPath>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(TargetFramework)|$(Platform)'=='Release|netcoreapp3.0|AnyCPU'">
    <OutputPath>..\..\..\Product\Release\</OutputPath>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(TargetFramework)|$(Platform)'=='Debug|net45|AnyCPU'">
    <OutputPath>..\..\..\Product\Debug\</OutputPath>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(TargetFramework)|$(Platform)'=='Release|net45|AnyCPU'">
    <OutputPath>..\..\..\Product\Release\</OutputPath>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\Base\HeBianGu.Base.WpfBase\HeBianGu.Base.WpfBase.csproj" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\..\Document\Resource\logo.jpg">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

  <Import Project="..\HeBianGu.General.WpfMvc.Share\HeBianGu.General.WpfMvc.Share.projitems" Label="Shared" />

</Project>
