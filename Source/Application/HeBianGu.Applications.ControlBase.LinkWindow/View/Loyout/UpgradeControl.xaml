<UserControl x:Class="HeBianGu.Applications.ControlBase.LinkWindow.View.Loyout.UpgradeControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:HeBianGu.Applications.ControlBase.LinkWindow.View.Loyout"
             xmlns:wpfcontrollib="clr-namespace:HeBianGu.General.WpfControlLib;assembly=HeBianGu.General.WpfControlLib"
             mc:Ignorable="d" 
             d:DesignHeight="1450" d:DesignWidth="800">
    <StackPanel>
        <GroupBox Header="程序升级">
            <StackPanel>
                <wpfcontrollib:FButton Content="检查更新"  Command="{Binding RelayCommand}" CommandParameter="Button.Upgrade"/>
            </StackPanel>
        </GroupBox>
    </StackPanel>
</UserControl>
