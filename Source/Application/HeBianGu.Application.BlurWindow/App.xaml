            </Style>-->

            <!--<Setter.Value>
                <h:BehaviorCollection>
                    <h:FountainAnimationBehavior IsUseAll="True"/>
                </h:BehaviorCollection>
            </Setter.Value>-->
            
            <local:DataSourceLocator x:Key="S.DataSource.Locator"/>

